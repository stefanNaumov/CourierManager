<Window x:Class="TeamWPF.Logging"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:my="clr-namespace:TeamWPF.Extensions"
        Title="Logging" Height="250" Width="300"
        Topmost="True">
    <Grid>
        <Label Content="Username" 
               Margin="19,45,193,143"
               HorizontalContentAlignment="Center"
               VerticalContentAlignment="Center">
        </Label>
        <TextBox Name="UsernameBox" 
                 Margin="104,45,66,143"
                 HorizontalContentAlignment="Left"
                 VerticalContentAlignment="Center"
                 FontSize="20">
        </TextBox>
        <Label Content="Password" 
               Margin="19,108,193,80" 
               HorizontalContentAlignment="Center"
               VerticalContentAlignment="Center">
        </Label>
        <TextBox Name="PasswordBox" 
                 Margin="104,108,66,80"
                 HorizontalContentAlignment="Left"
                 VerticalContentAlignment="Center"
                 FontSize="20">
        </TextBox>
        <Button 
            Content="Log In" 
            Margin="212,185,10,10" Click="Button_Click">
        </Button>
    </Grid>
</Window>
